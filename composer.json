{
    "name": "nswdpc/silverstripe-okta-boilerplate",
    "description": "An Okta authentication boilerplate for Silverstripe",
    "type": "silverstripe-vendormodule",
    "license": "BSD-3-Clause",
    "authors": [{
        "name": "James Ellis",
        "homepage": "https://dpc.nsw.gov.au",
        "role": "Developer"
    }],
    "keywords": [
        "silverstripe",
        "authentication",
        "okta",
        "oauth2",
        "okta php api"
    ],
    "extra": {
        "expose": [
            "client/static"
        ]
    },
    "require": {
        "php": ">=7.3 <8",
        "bigfork/silverstripe-oauth-login": "^2.1",
        "foxworth42/oauth2-okta": "^1",
        "okta/sdk": "^1.3",
        "silverstripe/framework": "^4.7",
        "phptek/jsontext": "^2",
        "symbiote/silverstripe-queuedjobs": "^4.5"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "phpunit/phpunit": "^5.7 | ^9.5",
        "web-token/jwt-key-mgmt" : "^2.2",
        "friendsofphp/php-cs-fixer": "^3",
        "silverstripe/sqlite3": "^2"
    },
    "autoload": {
        "psr-4": {
            "NSWDPC\\Authentication\\Okta\\": "src/",
            "NSWDPC\\Authentication\\Okta\\Tests\\": "tests/"
        }
    }
}
